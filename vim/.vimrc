
set nocompatible

syntax on

set autoindent

set smartindent 

set tabstop=4
set shiftwidth=4

set guioptions-=T
set vb t_vb=

set ruler

set nohls

set incsearch 

set background=dark

set showmode

set showcmd

set mouse=a

set showmatch

set matchtime=5

set ignorecase 

filetype indent on

set cursorline


colors  solarized

